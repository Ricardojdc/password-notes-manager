<UserControl x:Name="WelcomePageView" x:Class="Manager.Views.Welcome"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Manager.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="WelcomeGrid" Background="Black">
        <TextBlock x:Name="TextBlockWelcome" Text="This is the Home Page!" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" Foreground="White"/>
    </Grid>
</UserControl>
