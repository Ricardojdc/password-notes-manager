<Window x:Name="New_Entry_Window" x:Class="Manager.Views.PasswordPageWindows.NewEntryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Manager.Views.PasswordPageWindows"
        mc:Ignorable="d"
        Title="New Entry" Height="300" Width="500" MinHeight="300" MinWidth="500" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Background="#FF9C9C9C" MinWidth="500" MaxWidth="500" MinHeight="284" MaxHeight="284">
        <Label x:Name="Site" Content="Site" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="80,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166" Height="19" BorderBrush="White"/>
        <Label x:Name="Login" Content="Login" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="80,45,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="Password" Content="Password" Margin="10,72,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox HorizontalAlignment="Left" Margin="80,76,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
